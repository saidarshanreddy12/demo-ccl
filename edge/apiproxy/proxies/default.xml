<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <DefaultFaultRule name="fault-rule">
        <Step>
            <Name>FC-ErrorHandling</Name>
        </Step>
    </DefaultFaultRule>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AM-IntialSuccessFlag</Name>
            </Step>
            <Step>
                <Name>FC-PreflowPolicies</Name>
            </Step>
            <Step>
                <Name>OAS-RequestValidation</Name>
                <Condition>(proxy.pathsuffix MatchesPath "/customerlimit")</Condition>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <Flows>
        <Flow name="Customer_Limit_Equiry">
            <Description/>
            <Request>
                <Step>
                    <Name>FC-ThreatProtection</Name>
                </Step>
                <Step>
                    <Name>AM-SetRequest</Name>
                </Step>
            </Request>
            <Response>
                <Step>
                    <Name>AM-SetMockResponse</Name>
                </Step>
                <Step>
                    <Name>AM-FinalSuccessFlag</Name>
                </Step>
            </Response>
            <Condition>(proxy.pathsuffix MatchesPath "/customerlimit")</Condition>
        </Flow>
        <Flow name="Server Unavailable">
            <Description/>
            <Request>
                <Step>
                    <Name>RF-ResourceNotFound</Name>
                </Step>
            </Request>
            <Response/>
        </Flow>
    </Flows>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PostClientFlow>
        <Request/>
        <Response>
            <Step>
                <Name>ML-setMessageLogging</Name>
            </Step>
        </Response>
    </PostClientFlow>
    <HTTPProxyConnection>
        <BasePath>/ccl</BasePath>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>TargetEndpoint-1</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>