<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-setPayloadResponse">
    <DisplayName>AM-setPayloadResponse</DisplayName>
    <Properties/>
    <Set>
        <Payload contentType="application/json">{"ResponseSignatureEncryptedValue": "{ResponseSignatureEncryptedValue}","GWSymmetricKeyEncryptedValue": "{SymmetricKeyEncryptedValue}","Scope": "{requestPayload.Scope}","TransactionId": "{requestPayload.TransactionId}","Status": "SUCCESS"}</Payload>
    </Set>
    <AssignVariable>
        <Name>isSuccess</Name>
        <Value>true</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>responsetoclient</Name>
        <Ref>response.content</Ref>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response">response</AssignTo>
</AssignMessage>