<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-ResourceNotFound">
    <DisplayName>RF-ResourceNotFound</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json">
              {
                "request-id": "{req-requestId}",
                "errors": [
                {
                    "code": "404",
                    "detail": "Content Not Found",
                    "source": "apigw"
                }
                ]
               }
            </Payload>
            <StatusCode>404</StatusCode>
            <ReasonPhrase>Not Found</ReasonPhrase>
        </Set>
        <AssignVariable>
            <Name>apigw.code</Name>
            <Value>404</Value>
        </AssignVariable>
        <AssignVariable>
            <Name>apigw.message</Name>
            <Value>Content Not Found</Value>
        </AssignVariable>
        <AssignVariable>
            <Name>errorHandled</Name>
            <Value>true</Value>
        </AssignVariable>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>